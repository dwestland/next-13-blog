{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.format": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#0c345a",
    "activityBar.background": "#0c345a",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#c51a72",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#0c345a",
    "statusBar.background": "#061a2d",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0c345a",
    "statusBarItem.remoteBackground": "#061a2d",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#061a2d",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#061a2d99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#061a2d"
}
